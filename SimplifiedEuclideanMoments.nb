Cell["The G recursion", "Section",
 CellChangeTimes->{{3.522425455365881*^9, 3.522425459477894*^9}}],

Cell[BoxData[{
 RowBox[{
  RowBox[{"Clear", "[", "G", "]"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"G", "[", 
    RowBox[{"n_", ",", "0", ",", "0", ",", "z_"}], "]"}], ":=", "1"}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"G", "[", 
    RowBox[{"1", ",", "k_", ",", "m_", ",", "z_"}], "]"}], ":=", 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"z", "^", 
     RowBox[{"(", 
      RowBox[{
       RowBox[{"2", "*", "k"}], "+", "m"}], ")"}]}], "/", 
    RowBox[{"(", 
     RowBox[{
      RowBox[{"2", "*", "k"}], " ", "+", "m", "+", "1"}], ")"}]}]}], 
  ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"G", "[", 
    RowBox[{"n_", ",", "k_", ",", "m_", ",", "z_"}], "]"}], ":=", 
   RowBox[{
    RowBox[{"G", "[", 
     RowBox[{"n", ",", "k", ",", "m", ",", "z"}], "]"}], "=", 
    RowBox[{"Expand", "[", 
     RowBox[{"Sum", "[", 
      RowBox[{
       RowBox[{"Sum", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"Binomial", "[", 
           RowBox[{"k", ",", "j"}], "]"}], "*", 
          RowBox[{"Binomial", "[", 
           RowBox[{"m", ",", "jd"}], "]"}], "*", 
          RowBox[{
           RowBox[{"z", "^", 
            RowBox[{"(", 
             RowBox[{
              RowBox[{"2", "*", "j"}], "+", "jd"}], ")"}]}], "/", 
           RowBox[{"(", 
            RowBox[{
             RowBox[{"2", "*", "j"}], " ", "+", "jd", "+", "1"}], ")"}]}], 
          "*", 
          RowBox[{"G", "[", 
           RowBox[{
            RowBox[{"n", "-", "1"}], ",", 
            RowBox[{"k", "-", "j"}], ",", 
            RowBox[{"m", "-", "jd"}], ",", "z"}], "]"}]}], ",", 
         RowBox[{"{", 
          RowBox[{"jd", ",", "0", ",", "m"}], "}"}]}], "]"}], ",", 
       RowBox[{"{", 
        RowBox[{"j", ",", "0", ",", "k"}], "}"}]}], "]"}], "]"}]}]}], 
  ";"}]}], "Input",
 CellChangeTimes->{{3.52242547079017*^9, 3.522425591905157*^9}, {
  3.522425776106481*^9, 3.522425777838591*^9}}]
}, Open  ]],

