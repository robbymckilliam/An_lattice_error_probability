Cell["The B function", "Section",
 CellChangeTimes->{{3.522425630560517*^9, 3.522425633151152*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"B", "[", 
    RowBox[{"n_", ",", "t_", ",", "z_"}], "]"}], " ", ":=", " ", 
   RowBox[{"Sum", "[", 
    RowBox[{
     RowBox[{"Sum", "[", 
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{"(", 
          RowBox[{
           RowBox[{"-", "1"}], "/", 
           RowBox[{"(", 
            RowBox[{"n", "+", "1"}], ")"}]}], ")"}], "^", 
         RowBox[{"(", 
          RowBox[{"t", "-", "j1"}], ")"}]}], "*", 
        RowBox[{"2", "^", "j2"}], "*", 
        RowBox[{"z", "^", "j2"}], "*", 
        RowBox[{
         RowBox[{
          RowBox[{
           RowBox[{"Factorial", "[", "t", "]"}], "/", 
           RowBox[{"Factorial", "[", "j1", "]"}]}], "/", 
          RowBox[{"Factorial", "[", "j2", "]"}]}], "/", 
         RowBox[{"Factorial", "[", 
          RowBox[{"t", "-", "j1", "-", "j2"}], "]"}]}], "*", 
        RowBox[{"G", "[", 
         RowBox[{
          RowBox[{"n", "-", "1"}], ",", "j1", ",", 
          RowBox[{"j2", "+", 
           RowBox[{"2", "*", 
            RowBox[{"(", 
             RowBox[{"t", "-", "j1", "-", "j2"}], ")"}]}]}], ",", "z"}], 
         "]"}]}], ",", 
       RowBox[{"{", 
        RowBox[{"j2", ",", "0", ",", 
         RowBox[{"t", "-", "j1"}]}], "}"}]}], "]"}], ",", 
     RowBox[{"{", 
      RowBox[{"j1", ",", "0", ",", "t"}], "}"}]}], "]"}]}], ";"}]], "Input",
 CellChangeTimes->{{3.522425648601155*^9, 3.522425669998946*^9}}]
}, Open  ]],

