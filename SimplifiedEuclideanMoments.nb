      RowBox[{"B", "[", 
       RowBox[{"n", ",", 
        RowBox[{"m", "-", "k"}], ",", "z"}], "]"}]}], ",", 
     RowBox[{"{", 
      RowBox[{"k", ",", "0", ",", "m"}], "}"}]}], "]"}]}], ";", 
